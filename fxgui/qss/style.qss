QProgressBar:horizontal {
    border: 1px solid @GreyA;
    border-radius: 4px;
    text-align: center;
    padding: 1px;
    background: @GreyH;
    height: 8px;
}

QProgressBar::chunk:horizontal {
    border-radius: 3px;
    background-color: qlineargradient(spread:reflect, x1:0, y1:0.5, x2:1, y2:0.5, stop:0 @ColorB, stop:1 @ColorA);
}

QToolTip
{
    border: 1px solid @GreyK;
    background-color: @GreyH;
    color: @GreyE;
    padding: 4px 8px;
    border-radius: 4px;
}

QWidget
{
    color: @GreyE;
    background-color: @GreyC;
    selection-background-color: @ColorA;
    selection-color: white;
    background-clip: border;
    border-image: none;
    outline: 0;
    font-size: 12px;
}

QWidget:item:hover
{
    background-color: @ColorB;
    color: white;
}

QWidget:item:selected
{
    background-color: @ColorA;
}

QCheckBox
{
    spacing: 5px;
    outline: none;
    color: @GreyE;
    margin-bottom: 2px;
}

QCheckBox:disabled
{
    color: @GreyD;
}
QCheckBox::indicator,
QGroupBox::indicator
{
    width: 15px;
    height: 15px;
}
QGroupBox::indicator
{
    margin-left: 2px;
}

QCheckBox::indicator:unchecked,
QCheckBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked,
QGroupBox::indicator:unchecked:hover
{
    image: url(~icons/checkbox_unchecked.svg);
    width: 18px;
    height: 18px;
}

QCheckBox::indicator:unchecked:focus,
QCheckBox::indicator:unchecked:pressed,
QGroupBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:pressed
{
  border: none;
    image: url(~icons/checkbox_unchecked_focus.svg);
    width: 18px;
    height: 18px;
}

QCheckBox::indicator:checked,
QCheckBox::indicator:checked:hover,
QGroupBox::indicator:checked,
QGroupBox::indicator:checked:hover
{
    image: url(~icons/checkbox_checked.svg);
    width: 18px;
    height: 18px;
}

QCheckBox::indicator:checked:focus,
QCheckBox::indicator:checked:pressed,
QGroupBox::indicator:checked:focus,
QGroupBox::indicator:checked:pressed
{
  border: none;
    image: url(~icons/checkbox_checked_focus.svg);
    width: 18px;
    height: 18px;
}

QCheckBox::indicator:indeterminate,
QCheckBox::indicator:indeterminate:hover,
QCheckBox::indicator:indeterminate:pressed
QGroupBox::indicator:indeterminate,
QGroupBox::indicator:indeterminate:hover,
QGroupBox::indicator:indeterminate:pressed
{
    image: url(~icons/checkbox_indeterminate.svg);
    width: 18px;
    height: 18px;
}

QCheckBox::indicator:indeterminate:focus,
QGroupBox::indicator:indeterminate:focus
{
    image: url(~icons/checkbox_indeterminate_focus.svg);
    width: 18px;
    height: 18px;
}

QCheckBox::indicator:checked:disabled,
QGroupBox::indicator:checked:disabled
{
    image: url(~icons/checkbox_checked_disabled.svg);
    width: 18px;
    height: 18px;
}

QCheckBox::indicator:unchecked:disabled,
QGroupBox::indicator:unchecked:disabled
{
    image: url(~icons/checkbox_unchecked_disabled.svg);
    width: 18px;
    height: 18px;
}

QRadioButton
{
    spacing: 5px;
    outline: none;
    color: @GreyE;
    margin-bottom: 2px;
}

QRadioButton:disabled
{
    color: @GreyD;
}
QRadioButton::indicator
{
    width: 21px;
    height: 21px;
}

QRadioButton::indicator:unchecked,
QRadioButton::indicator:unchecked:hover
{
    image: url(~icons/radio_unchecked.svg);
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:unchecked:focus,
QRadioButton::indicator:unchecked:pressed
{
  border: none;
  outline: none;
    image: url(~icons/radio_unchecked_focus.svg);
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:checked,
QRadioButton::indicator:checked:hover
{
  border: none;
  outline: none;
    image: url(~icons/radio_checked.svg);
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:checked:focus,
QRadioButton::indicator:checked:pressed
{
  border: none;
  outline: none;
    image: url(~icons/radio_checked_focus.svg);
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:indeterminate,
QRadioButton::indicator:indeterminate:hover,
QRadioButton::indicator:indeterminate:pressed
{
    image: url(~icons/radio_indeterminate.svg);
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:checked:disabled
{
    outline: none;
    image: url(~icons/radio_checked_disabled.svg);
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:unchecked:disabled
{
    image: url(~icons/radio_unchecked_disabled.svg);
    width: 18px;
    height: 18px;
}


QMenuBar
{
    background-color: @GreyH;
    border-bottom: 1px solid @GreyP;
}

QMenuBar::item
{
    background: transparent;
    padding: 10px;
}

QMenuBar::item:selected
{
    color: white;
    background-color: @ColorA;
    border-radius: 2px;
    margin: 5px;
    /* background: transparent; */
    /* border: 1px solid @GreyA; */
}

QMenuBar::item:pressed
{
    border: 1px solid @GreyA;
    background-color: @ColorB;
    color: white;
    padding-bottom:8px;
}

QMenu
{
    border: 1px solid @GreyA;
    border-radius: 2px;
    color: silver;
    /* margin: 2px; */
    background-color: @GreyH;
}

QMenu::icon
{
    margin-left: 25px;
}

QMenu::item
{
    background-color: @GreyH;
    border-radius: 2px;
    padding: 5px 25px 5px 25px;
    border: 3px transparent;
}

QMenu::item:selected
{
    color: white;
}

QMenu::item:pressed
{
    color: white;
    background-color: @ColorB;
}

QMenu::separator {
    height: 1px;
    background-color: @GreyA;
    margin-left: 10px;
    margin-right: 5px;
}

QMenu::indicator {
    width: 18px;
    height: 18px;
}

/* non-exclusive indicator = check box style indicator
   (see QActionGroup::setExclusive) */
QMenu::indicator:non-exclusive:unchecked {
    image: url(~icons/checkbox_unchecked.svg);
    width: 18px;
    height: 18px;
}

QMenu::indicator:non-exclusive:unchecked:selected {
    image: url(~icons/checkbox_unchecked_focus.svg);
    width: 18px;
    height: 18px;
}

QMenu::indicator:non-exclusive:checked {
    image: url(~icons/checkbox_checked.svg);
    width: 18px;
    height: 18px;
}

QMenu::indicator:non-exclusive:checked:selected {
    image: url(~icons/checkbox_checked_focus.svg);
    width: 18px;
    height: 18px;
}

/* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
QMenu::indicator:exclusive:unchecked {
    image: url(~icons/radio_unchecked.svg);
    width: 18px;
    height: 18px;
}

QMenu::indicator:exclusive:unchecked:selected {
    image: url(~icons/radio_unchecked_focus.svg);
    width: 18px;
    height: 18px;
}

QMenu::indicator:exclusive:checked {
    image: url(~icons/radio_checked.svg);
    width: 18px;
    height: 18px;
}

QMenu::indicator:exclusive:checked:selected {
    image: url(~icons/radio_checked_focus.svg);
    width: 18px;
    height: 18px;
}

QMenu::right-arrow {
    margin: 5px;
    image: url(~icons/right_arrow.svg)
}


QWidget:disabled
{
    color: @GreyF;
    background-color: @GreyC;
}

QAbstractItemView
{
    alternate-background-color: @GreyB;
    color: silver;
    border: 1px solid @GreyB;
    border-radius: 2px;
    padding: 1px;
}

QWidget:focus,
QMenuBar:focus
{
    border: none;
}

QTabWidget:focus, QCheckBox:focus,
QRadioButton:focus,
QSlider:focus
{
    border: none;
}

QLineEdit
{
    background-color: @GreyH;
    padding: 4px 6px;
    border-style: solid;
    border: 1px solid @GreyA;
    border-radius: 4px;
    color: @GreyE;
}

QGroupBox {
    border:1px solid @GreyA;
    border-radius: 2px;
    margin-top: 20px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
}

QAbstractScrollArea
{
    border-radius: 2px;
    border: 1px solid @GreyA;
    background-color: transparent;
}

QScrollBar:horizontal
{
    height: 12px;
    margin: 0px 12px 0px 12px;
    border: none;
    border-radius: 6px;
    background-color: @GreyP;
}

QScrollBar::handle:horizontal
{
    background-color: @GreyQ;
    min-width: 20px;
    border-radius: 4px;
    margin: 2px;
}

QScrollBar::handle:horizontal:hover
{
    background-color: @GreyN;
}

QScrollBar::add-line:horizontal
{
    margin: 0px 3px 0px 3px;
    border-image: url(~icons/right_arrow_disabled.svg);
    width: 10px;
    height: 10px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal
{
    margin: 0px 3px 0px 3px;
    border-image: url(~icons/left_arrow_disabled.svg);
    height: 10px;
    width: 10px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover,
QScrollBar::add-line:horizontal:on
{
    border-image: url(~icons/right_arrow.svg);
    height: 10px;
    width: 10px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}


QScrollBar::sub-line:horizontal:hover,
QScrollBar::sub-line:horizontal:on
{
    border-image: url(~icons/left_arrow.svg);
    height: 10px;
    width: 10px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal
{
    background: none;
}


QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal
{
    background: none;
}

QScrollBar:vertical
{
    background-color: @GreyP;
    width: 12px;
    margin: 12px 0px 12px 0px;
    border: none;
    border-radius: 6px;
}

QScrollBar::handle:vertical
{
    background-color: @GreyQ;
    min-height: 20px;
    border-radius: 4px;
    margin: 2px;
}

QScrollBar::handle:vertical:hover
{
    background-color: @GreyN;
}

QScrollBar::sub-line:vertical
{
    margin: 3px 0px 3px 0px;
    border-image: url(~icons/up_arrow_disabled.svg);
    height: 10px;
    width: 10px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical
{
    margin: 3px 0px 3px 0px;
    border-image: url(~icons/down_arrow_disabled.svg);
    height: 10px;
    width: 10px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover,
QScrollBar::sub-line:vertical:on
{

    border-image: url(~icons/up_arrow.svg);
    height: 10px;
    width: 10px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}


QScrollBar::add-line:vertical:hover,
QScrollBar::add-line:vertical:on
{
    border-image: url(~icons/down_arrow.svg);
    height: 10px;
    width: 10px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical
{
    background: none;
}


QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical
{
    background: none;
}

QTextEdit
{
    background-color: @GreyH;
    color: silver;
    border: 1px solid @GreyA;
}

QPlainTextEdit
{
    background-color: @GreyH;
    color: silver;
    border-radius: 2px;
    border: 1px solid @GreyA;
}

QSizeGrip {
    background: transparent;
    /* image: url(~icons/sizegrip.svg); */
    width: 0;
    height: 0;
}


QMainWindow::separator
{
    background-color: @GreyC;
    color: white;
    padding-left: 4px;
    spacing: 2px;
    border: 1px dashed @GreyA;
}

QMainWindow::separator:hover
{

    background-color: @GreyG;
    color: white;
    padding-left: 4px;
    border: 1px solid @GreyA;
    spacing: 2px;
}


QMenu::separator
{
    height: 1px;
    background-color: @GreyA;
    color: white;
    padding-left: 4px;
    margin-left: 10px;
    margin-right: 5px;
}


QFrame
{
    border-radius: 2px;
    /* border: 1px solid @GreyJ; */
}

QFrame[frameShape="0"]
{
    border-radius: 2px;
    border: 1px transparent @GreyJ;
}

QStackedWidget
{
    border: 1px transparent;
}

QToolBar {
    border: 1px transparent @GreyJ;
    background: 1px solid @GreyC;
    font-weight: bold;
    border-bottom: 1px solid @GreyA;
}

QToolBar::handle:horizontal {
    image: url(~icons/horizontal_move_toolbar.svg);
    width: 18px;
    height: 18px;
}
QToolBar::handle:vertical {
    image: url(~icons/vertical_move_toolbar.svg);
    width: 18px;
    height: 18px;
}
QToolBar::separator:horizontal {
    image: url(~icons/horizontal_separator_toolbar.svg);
    width: 18px;
    height: 18px;
}
QToolBar::separator:vertical {
    image: url(~icons/vertical_separator_toolbar.svg);
    width: 18px;
    height: 18px;
}

QPushButton
{
    color: @GreyE;
    background-color: @GreyC;
    border-width: 1px;
    border-color: @GreyK;
    border-style: solid;
    padding-top: 5px;
    padding-bottom: 5px;
    padding-left: 10px;
    padding-right: 10px;
    border-radius: 4px;
    outline: none;
}

QPushButton:disabled
{
    background-color: @GreyC;
    border-width: 1px;
    border-color: @GreyA;
    border-style: solid;
    padding-top: 5px;
    padding-bottom: 5px;
    padding-left: 10px;
    padding-right: 10px;
    /*border-radius: 2px;*/
    color: @GreyJ;
}

QPushButton:focus {
    border: 1px solid @GreyK;
}

QComboBox
{
    selection-background-color: @ColorA;
    background-color: @GreyH;
    border-style: solid;
    border: 1px solid @GreyA;
    border-radius: 4px;
    padding: 4px 6px;
    min-width: 75px;
}

QPushButton:checked{
    background-color: @GreyK;
    border-color: @GreyI;
}

QComboBox:hover,
QPushButton:hover,
QAbstractSpinBox:hover,
QLineEdit:hover,
QTextEdit:hover,
QPlainTextEdit:hover,
QAbstractView:hover,
QTreeView:hover,
QListView:hover
{
    border: 1px solid @ColorA;
    color: silver;
}


QComboBox:on
{
    /* background-color: #626873; */
    padding-top: 3px;
    padding-left: 4px;
    /* selection-background-color: #4a4a4a; */
}

QComboBox QAbstractItemView
{
    background-color: @GreyH;
    border-radius: 2px;
    border: 1px solid @GreyJ;
    selection-background-color: @ColorA;
}

QComboBox::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-left-width: 0px;
    border-left-color: darkgray;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow
{
    image: url(~icons/down_arrow_disabled.svg);
}

QComboBox::down-arrow:on,
QComboBox::down-arrow:hover,
QComboBox::down-arrow:focus
{
    image: url(~icons/down_arrow.svg);
}

QPushButton:pressed
{
    background-color: @GreyK;
}

QAbstractSpinBox {
    padding-top: 2px;
    padding-bottom: 2px;
    border: 1px solid @GreyA;
    background-color: @GreyH;
    color: silver;
    border-radius: 2px;
    min-width: 75px;
}

QAbstractSpinBox:up-button
{
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: center right;
}

QAbstractSpinBox:down-button
{
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: center left;
}

QAbstractSpinBox::up-arrow,
QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off {
    image: url(~icons/up_arrow_disabled.svg);
    width: 10px;
    height: 10px;
}
QAbstractSpinBox::up-arrow:hover
{
    image: url(~icons/up_arrow.svg);
}

QAbstractSpinBox::down-arrow,
QAbstractSpinBox::down-arrow:disabled,
QAbstractSpinBox::down-arrow:off
{
    image: url(~icons/down_arrow_disabled.svg);
    width: 10px;
    height: 10px;
}
QAbstractSpinBox::down-arrow:hover
{
    image: url(~icons/down_arrow.svg);
}

QLabel
{
    border: 0px solid;
}

QTabWidget{
    border: 1px transparent;
}

QTabWidget::pane {
    top:-1px;
    border: 1px solid @GreyK;
    border-radius: 3px;
    padding: 3px;
}

QTabBar
{
    qproperty-drawBase: 0;
    left: 5px; /* Move to the right by 5px */
}

QTabBar:focus
{
    border: 0px transparent;
}

QTabBar QToolButton::right-arrow:enabled {
    image: url(~icons/right_arrow.svg);
}

 QTabBar QToolButton::left-arrow:enabled {
    image: url(~icons/left_arrow.svg);
}

QTabBar QToolButton::right-arrow:disabled {
    image: url(~icons/right_arrow_disabled.svg);
}

QTabBar QToolButton::left-arrow:disabled {
    image: url(~icons/left_arrow_disabled.svg);
}

QTabBar::close-button  {
    image: url(~icons/close.svg);
    background: transparent;
}

QTabBar::close-button:hover
{
    image: url(~icons/close_hover.svg);
    background: transparent;
}

QTabBar::close-button:pressed {
    image: url(~icons/close_pressed.svg);
    background: transparent;
}

QTabBar::close-button:selected {
    margin-bottom: 1px;
}

QTabBar::close-button:!selected {
    border: 1px transparent;
    border-bottom: 1px transparent;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    margin-top: 3px;
}

/* TOP TABS */
QTabBar::tab:top {
    color: @GreyE;
    border: 1px solid @GreyK;
    border-bottom: 1px transparent;
    background-color: @GreyC;
    padding: 5px 20px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 75px;
}

QTabBar::tab:top:selected {
    color: white;
    background-color: @GreyC;
    border-bottom: 1px solid @GreyC;
}

QTabBar::tab:top:!selected {
    color: @GreyL;
    background-color: @GreyH;
    border: 1px solid @GreyK;
    border-bottom: 1px solid @GreyK;
    margin-top: 2px;
}

QTabBar::tab:top:!selected:hover {
    color: white;
    background-color: @GreyK;
}

/* LEFT TABS */
QTabBar::tab:left {
    color: @GreyE;
    border: 1px solid @GreyK;
    border-right: 1px transparent;
    background-color: @GreyC;
    padding: 5px 20px;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    min-width: 75px;
}

QTabBar::tab:left:selected {
    color: white;
    background-color: @GreyC;
    border-right: 1px solid @GreyC;
}

QTabBar::tab:left:!selected {
    color: @GreyL;
    background-color: @GreyH;
    border: 1px solid @GreyK;
    border-right: 1px solid @GreyK;
    margin-left: 2px;
}

QTabBar::tab:left:!selected:hover {
    color: white;
    background-color: @GreyK;
}

/* BOTTOM TABS */
QTabBar::tab:bottom {
    color: @GreyE;
    border: 1px solid @GreyK;
    border-top: 1px transparent;
    background-color: @GreyC;
    padding: 5px 20px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    min-width: 75px;
}

QTabBar::tab:bottom:selected {
    color: white;
    background-color: @GreyC;
    border-top: 1px solid @GreyC;
}

QTabBar::tab:bottom:!selected {
    color: @GreyL;
    background-color: @GreyH;
    border: 1px solid @GreyK;
    border-top: 1px solid @GreyK;
    margin-bottom: 2px;
}

QTabBar::tab:bottom:!selected:hover {
    color: white;
    background-color: @GreyK;
}

/* RIGHT TABS */
QTabBar::tab:right {
    color: @GreyE;
    border: 1px solid @GreyK;
    border-left: 1px transparent;
    background-color: @GreyC;
    padding: 5px 20px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    min-width: 75px;
}

QTabBar::tab:right:selected {
    color: white;
    background-color: @GreyC;
    border-left: 1px solid @GreyC;
}

QTabBar::tab:right:!selected {
    color: @GreyL;
    background-color: @GreyH;
    border: 1px solid @GreyK;
    border-left: 1px solid @GreyK;
    margin-right: 2px;
}

QTabBar::tab:right:!selected:hover {
    color: white;
    background-color: @GreyK;
}


QDockWidget {
    border: 1px solid @GreyO;
    titlebar-close-icon: url(~icons/close.svg);
    titlebar-normal-icon: url(~icons/undock.svg);
}

QDockWidget::close-button, QDockWidget::float-button {
    border: 1px solid transparent;
    border-radius: 2px;
    background: transparent;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background: rgba(255, 255, 255, 10);
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    padding: 1px -1px -1px 1px;
    background: rgba(255, 255, 255, 10);
}


QTreeView, QListView
{
    border: 1px solid @GreyJ;
    background-color: @GreyH;
}

/* QTreeView:branch:selected, QTreeView:branch:hover
{
    background: url(~icons/transparent.png);
}

QTreeView::branch:has-siblings:!adjoins-item {
    border-image: url(~icons/transparent.png);
}

QTreeView::branch:has-siblings:adjoins-item {
    border-image: url(~icons/transparent.png);
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
    border-image: url(~icons/transparent.png);
} */

QTreeView:branch:selected, QTreeView:branch:hover
{
    background: url(~icons/transparent.png);
}

QTreeView::branch:has-siblings:!adjoins-item {
    border-image: url(~icons/branch_vertical.svg);
}

QTreeView::branch:has-siblings:adjoins-item {
    border-image: url(~icons/branch_more.svg);
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
    border-image: url(~icons/branch_end.svg);
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    border-image: none;
    image: url(~icons/branch_closed.svg);
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings  {
    border-image: none;
    image: url(~icons/branch_open.svg);
}

QTreeView::branch:has-children:!has-siblings:closed:hover,
QTreeView::branch:closed:has-children:has-siblings:hover {
    border-image: none;
    image: url(~icons/branch_closed_on.svg);
}

QTreeView::branch:open:has-children:!has-siblings:hover,
QTreeView::branch:open:has-children:has-siblings:hover  {
    border-image: none;
    image: url(~icons/branch_open_on.svg);
}


QListView::item:!selected:hover,
QListView::item:!selected:hover,
QTreeView::item:!selected:hover  {
    background: @ColorB;
    outline: 0;
    color: white
}

QListView::item:selected:hover,
QListView::item:selected:hover,
QTreeView::item:selected:hover  {
    background: @ColorA;
    color: white;
}

QSlider::groove:horizontal {
    border: none;
    height: 6px;
    background: @GreyH;
    margin: 2px 0;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: @SliderHandle;
    border: 1px solid @GreyK;
    width: 14px;
    height: 14px;
    margin: -4px 0;
    border-radius: 7px;
}

QSlider::handle:horizontal:hover {
    background: @ColorA;
}

QSlider::sub-page:horizontal {
    border: none;
    height: 6px;
    background: qlineargradient(spread:pad, x1:0, y1:0.5, x2:1, y2:0.5, stop:0 @ColorB, stop:1 @ColorA);
    margin: 2px 0;
    border-radius: 3px;
}

QSlider::groove:vertical {
    border: none;
    width: 6px;
    background: @GreyH;
    margin: 0 0px;
    border-radius: 3px;
}

QSlider::handle:vertical {
    background: @SliderHandle;
    border: 1px solid @GreyK;
    width: 14px;
    height: 14px;
    margin: 0 -4px;
    border-radius: 7px;
}

QSlider::handle:vertical:hover {
    background: @ColorA;
}

QToolButton {
    background-color: transparent;
    border: 1px transparent @GreyK;
    border-radius: 2px;
    margin: 3px;
    padding: 3px;
}

QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
 padding-right: 20px; /* make way for the popup button */
 border: 1px transparent @GreyK;
 border-radius: 5px;
}

QToolButton[popupMode="2"] { /* only for InstantPopup */
 padding-right: 10px; /* make way for the popup button */
 border: 1px transparent @GreyK;
}

QToolButton:hover,
QToolButton::menu-button:hover {
    background-color: transparent;
    border: 1px solid @ColorA;
}

QToolButton:checked,
QToolButton:pressed,
        QToolButton::menu-button:pressed {
    background-color: @GreyK;
    border: 1px solid @ColorA;
}

/* the subcontrol below is used only in the InstantPopup or DelayedPopup mode */
QToolButton::menu-indicator {
    image: url(~icons/down_arrow.svg);
    top: -7px; left: -2px; /* shift it a bit */
}

/* the subcontrols below are used only in the MenuButtonPopup mode */
QToolButton::menu-button {
    border: 1px transparent @GreyK;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    /* 16px width + 4px for border = 20px allocated above */
    width: 16px;
    outline: none;
}

QToolButton::menu-arrow {
    image: url(~icons/down_arrow.svg);
}

QToolButton::menu-arrow:open {
    top: 1px; left: 1px; /* shift it a bit */
    border: 1px solid @GreyA;
}


QPushButton::menu-indicator  {
    subcontrol-origin: padding;
    subcontrol-position: right;
    width: 8px;
    height: 8px;
    padding-right: 5px;
}

QPushButton::menu-indicator:disabled {
    image: url(~icons/down_arrow_disabled.svg);
    width: 9px;
    height: 9px;
    padding-right: 3px;
}


QTableView
{
    border: 1px solid @GreyJ;
    gridline-color: @GreyI;
    background-color: @GreyH;
}


QTableView, QHeaderView
{
    border-radius: 0px;
}

QTableView::item:pressed,
QListView::item:pressed,
QTreeView::item:pressed  {
    border-color: @ColorA;
    background: @ColorA;
    color: white;
}

QTableView::item:selected:active,
QTreeView::item:selected:active,
QListView::item:selected:active  {
    border-color: @ColorB;
    background: @ColorA;
    color: white;
}


QHeaderView
{
    border: 1px transparent;
    border-radius: 2px;
    margin: 0px;
    padding: 0px;
}

QHeaderView::section  {
    background-color: @GreyA;
    /*color: silver;*/
    padding: 4px;
    border: 1px solid @GreyI;
    border-radius: 0px;
    text-align: center;
}

QHeaderView::section::vertical::first,
QHeaderView::section::vertical::only-one
{
    border-top: 1px solid @GreyI;
}

QHeaderView::section::vertical
{
    border-top: transparent;
}

QHeaderView::section::horizontal::first,
QHeaderView::section::horizontal::only-one
{
    border-left: 1px solid @GreyI;
}

QHeaderView::section::horizontal
{
    border-left: transparent;
}

QHeaderView::section:checked
{
    color: white;
    background-color: @GreyQ;
}

/* style the sort indicator */
QHeaderView::down-arrow {
    image: url(~icons/down_arrow.svg);
    width: 7px;
    height: 7px;
    margin-right: 7px;
}

QHeaderView::up-arrow {
    image: url(~icons/up_arrow.svg);
    width: 7px;
    height: 7px;
    margin-right: 7px;
}

QTableCornerButton::section {
    background-color: @GreyA;
    border: 1px solid @GreyA;
    border-radius: 2px;
}


QToolBox  {
    padding: 3px;
    border: 1px transparent;
}

QToolBox::tab {
    color: @GreyL;
    background-color: @GreyH;
    border: 1px solid @GreyK;
    border-bottom: 1px transparent @GreyC;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
}

QToolBox::tab:selected { /* Bold selected tabs */
    font: bold;
    background-color: @GreyC;
    border-color: @ColorA;
}


QStatusBar {
    background-color: @GreyH;
    border-top: 1px solid @GreyP;
}

QStatusBar::item {
    background: transparent;
    border: 1px transparent @GreyA;
}

QStatusBar QLabel {
    background: transparent;
    margin-top: 2px;
    margin-bottom: 2px;
}


QFrame[height="3"], QFrame[width="3"] {
    background-color: @GreyJ;
}


QSplitter::handle {
    border: 1px dashed @GreyA;
}

QSplitter::handle:hover {
    background-color: @GreyG;
    border: 1px solid @GreyA;
}

QSplitter::handle:horizontal {
    width: 1px;
